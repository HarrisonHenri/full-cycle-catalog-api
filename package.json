{
  "name": "full-cycle-catalog-api",
  "version": "0.0.1",
  "description": "full cycle catalog api",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "12 || 14 || 16 || 17"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn run eslint && yarn run prettier:check",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint --fix",
    "pretest": "yarn run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "yarn run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && yarn run posttest",
    "docker:build": "docker build -t full-cycle-catalog-api .",
    "docker:run": "docker run -p 3000:3000 -d full-cycle-catalog-api",
    "premigrate": "yarn run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "yarn run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "yarn run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "yarn run clean && yarn run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "HarrisonHenri <harrisonhenrisn@gmail.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^1.7.4",
    "@loopback/context": "^2.1.1",
    "@loopback/core": "^1.12.4",
    "@loopback/openapi-v3": "^2.0.0",
    "@loopback/repository": "^1.19.1",
    "@loopback/rest": "^2.0.0",
    "@loopback/rest-explorer": "^1.4.10",
    "@loopback/service-proxy": "^1.3.17",
    "loopback-connector-esv6": "https://github.com/codeedu/loopback-connector-elastic-search/tarball/master",
    "dotenv": "^10.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^3.1.1",
    "@loopback/eslint-config": "^5.0.3",
    "@loopback/testlab": "^1.10.3",
    "@types/node": "^10.17.14",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-eslint-plugin": "^2.2.1",
    "eslint-plugin-mocha": "^6.2.2",
    "source-map-support": "^0.5.16",
    "typescript": "~3.7.5"
  },
  "nodemonConfig": {
    "verbose": true,
    "watch": [
      "src/"
    ],
    "ext": ".ts",
    "exec": "yarn start"
  }
}
